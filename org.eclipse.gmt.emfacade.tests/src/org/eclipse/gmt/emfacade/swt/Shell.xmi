<?xml version="1.0" encoding="ASCII"?>
<swt:Shell xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:swt="org.eclipse.gmt.emfacade.swt" borderStyle="BORDER" size="400,400" modalStyle="PRIMARY_MODAL" trimStyle="SHELL_TRIM">
  <controls xsi:type="swt:Composite" size="200,200">
    <controls xsi:type="swt:Label" text="A label"/>
    <controls xsi:type="swt:Button" text="A toggle button"/>
    <controls xsi:type="swt:Button" text="A check button" buttonStyle="CHECK" selection="true"/>
    <controls xsi:type="swt:Button" text="A radio button" buttonStyle="RADIO"/>
    <controls xsi:type="swt:Text" borderStyle="BORDER" text="A text field"/>
    <layout xsi:type="swt:FillLayout" orientationStyle="VERTICAL"/>
  </controls>
  <controls xsi:type="swt:Composite">
    <controls xsi:type="swt:Combo">
      <items>First</items>
      <items>Third</items>
    </controls>
    <controls xsi:type="swt:List">
      <items>First</items>
      <items>Second</items>
      <items>Third</items>
      <items>Fourth</items>
      <items>Fifth</items>
    </controls>
    <layout xsi:type="swt:FillLayout" orientationStyle="VERTICAL"/>
  </controls>
  <layout xsi:type="swt:FillLayout"/>
  <menuBar>
    <items text="&amp;File" menuItemStyle="CASCADE">
      <menu menuStyle="DROP_DOWN">
        <items text="Open"/>
        <items text="Save"/>
      </menu>
    </items>
  </menuBar>
</swt:Shell>
